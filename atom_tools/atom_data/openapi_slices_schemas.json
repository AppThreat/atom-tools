              "code": {
                "description": "Raw source code of the method",
                "type": "string"
              },
              "columnNumber": {
                "description": "Column number",
                "format": "int32",
                "type": "integer"
              },
              "fileName": {
                "description": "File name",
                "type": "string"
              },
              "fullName": {
